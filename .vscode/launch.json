{
  "version": "1.0.0",
  "configurations": [
    {
      "name": "Run File",
      "type": "node",
      "runtimeVersion": "22",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceRoot}",
      "envFile": "${workspaceFolder}/.env",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ],
    }
  ]
}