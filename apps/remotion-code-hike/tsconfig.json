{
  "compilerOptions": {
    "target": "ES2018",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
    "lib": [
      "es2015",
      "dom",
      "DOM.Iterable"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
  }
}