{
  "name": "written-content-manager",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev --port 3004",
    "local-db": "docker-compose up -d",
    "lint": "tsc",
    "start": "remix-serve ./build/server/index.js",
    "prisma:generate": "prisma generate",
    "db:dev": "vitest",
    "migrate:dev": "DATABASE_URL=\"postgresql://postgres@localhost:5432/postgres\" prisma migrate dev",
    "migrate:prod": "prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "5.19.1",
    "@remix-run/node": "2.11.1",
    "@remix-run/react": "2.11.1",
    "@remix-run/serve": "2.11.1",
    "prisma": "^5.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "2.11.1",
    "@total-typescript/shared": "workspace:*",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.16.2",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
